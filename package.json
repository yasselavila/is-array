{
  "name": "@yag/is-array",
  "description": "Just a fallback for Array.isArray()",
  "version": "1.0.1",
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "node": ">=4.2.0"
  },
  "homepage": "https://github.com/yasselavila/is-array",
  "author": {
    "name": "Yassel Avila Gil",
    "url": "http://yasselavila.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yasselavila/is-array.git"
  },
  "keyworks": [
    "yag",
    "array",
    "is-array"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "start": "npm run test:w",
    "cover": "cross-env NODE_ENV=test nyc npm test",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register src/**/*.spec.ts",
    "test:w": "cross-env NODE_ENV=test mocha --watch -r ts-node/register src/**/*.spec.ts",
    "tsc": "tsc --outDir dist",
    "tsc:w": "npm run tsc -- -w",
    "bundle": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "bundle:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "clean": "rimraf dist && mkdir dist",
    "pkg:data": "cpx {package.json,README.md,LICENSE.txt} dist/",
    "build": "run-s clean cover pkg:data tsc bundle"
  },
  "devDependencies": {
    "@types/node": "~8.0.53",
    "@types/mocha": "^2.2.43",
    "@types/chai": "^4.0.5",
    "rimraf": "^2.6.2",
    "npm-run-all": "^4.1.2",
    "cross-env": "^5.1.0",
    "webpack": "^3.8.1",
    "awesome-typescript-loader": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "mocha": "^4.0.1",
    "chai": "^4.1.2",
    "nyc": "^11.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.6.1"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      ".vscode",
      "coverage",
      "dist",
      "node_modules",
      "typings",
      "manual-tests"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
